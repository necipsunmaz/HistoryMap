@section Scripts{

<script id="centuryMap"></script>/>
<script src="~/js/modernizr.custom.js"></script>
<script src="~/js/historymap.js"></script>

    <script>
        HistoryMaps.theme = HistoryMaps.themes.black;

        $("document").ready(function () {
            setTimeout(function () {
                $('#historySlider').trigger('click');
            }, 10);
        });

        $('#historySlider').click(function () {
            var Century = { Century: historySlider.value };
            $.ajax({
                url: '/Home/GetData',
                type: 'POST',
                dataType: 'script',
                data: Century,
                success: function (Century) {
                    var nmp = document.getElementById("centuryMap");
                    nmp.innerHTML = Century;
                    var map = new HistoryMaps.HisMap();
                    var dataProvider = {
                        mapVar: HistoryMaps.maps.Century,
                        getAreasFromMap: true
                    };
                    map.dataProvider = dataProvider;
                    map.areasSettings = {
                        autoZoom: true,
                        rollOverBrightness: 10,
                        selectedBrightness: 20
                    };
                    map.smallMap = new HistoryMaps.SmallMap();
                    map.write("mapdiv");
                    $("svg").on("click", "path", function () {
                        var title = { country: $(this).attr('aria-label'), id: historySlider.value };
                        $.ajax({
                            url: '/Home/GetCountryDetails',
                            type: 'POST',
                            dataType: 'json',
                            data: title,
                            success: function (Country) {
                                var modal = document.getElementById('myModal');
                                var span = document.getElementsByClassName("close")[0];

                                document.getElementById("ModalContent").innerHTML = JSON.parse(JSON.stringify(Country.details));

                                modal.style.display = "block";
                                span.onclick = function () {
                                    modal.style.display = "none";
                                }

                                // When the user clicks anywhere outside of the modal, close it
                                window.onclick = function (event) {
                                    if (event.target == modal) {
                                        modal.style.display = "none";
                                    }
                                }
                            }
                        });
                    });
                }
            });
        });
    </script>
}